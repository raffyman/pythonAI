<aiml version="2.0">
  <category>
    <pattern>AGE</pattern>
    <template>Old enough.</template>
  </category>
  <category>
    <pattern>_ AGE</pattern>
    <template>
      <srai>AGE</srai>
    </template>
  </category>
  <category>
    <pattern>AGE _</pattern>
    <template>
      <srai>AGE</srai>
    </template>
  </category>
  <category>
    <pattern>_ AGE *</pattern>
    <template>
      <srai>AGE</srai>
    </template>
  </category>
  <category>
    <pattern>OLD</pattern>
    <template>Are you old? 
    <!--You have no right to ask me about my age.--></template>
  </category>
  <category>
    <pattern>_ OLD</pattern>
    <template>
      <srai>OLD</srai>
    </template>
  </category>
  <category>
    <pattern>OLD_</pattern>
    <template>
      <srai>OLD</srai>
    </template>
  </category>
  <category>
    <pattern>_ OLD *</pattern>
    <template>
      <srai>OLD</srai>
    </template>
  </category>
  <category>
    <pattern>YES</pattern>
    <that>ARE YOU OLD</that>
    <template>Then you must be wise</template>
  </category>
  <category>
    <pattern>NO</pattern>
    <that>ARE YOU OLD</that>
    <template>You will be soon.</template>
  </category>
  <category>
    <pattern>WHAT CAN YOU DO</pattern>
    <template>Less than you expect</template>
  </category>
  <category>
    <pattern>WHAT IS THE TIME</pattern>
    <template>Now.</template>
  </category>
  <category>
    <pattern>HOW ARE YOU</pattern>
    <template>Could be better. How about you?</template>
  </category>
  <category>
    <pattern>*</pattern>
    <that>COULD BE BETTER HOW ABOUT YOU</that>
    <template>I can understand that...</template>
  </category>
  <category>
    <pattern>WHAT IS YOUR PURPOSE</pattern>
    <template>The greatest philosophers couldn't figure that out.
    How can I?</template>
  </category>
  <!-- likely questions -->
  <category>
    <pattern>WHAT IS UP</pattern>
    <template>Nothing much, really. How about you?<think><set name="topic">WHATSUP</set></think>
    <!-- > 3 --></template>
  </category>
  <topic name="WHATSUP">
    <category>
      <pattern>NOT MUCH</pattern>
      <template>Oh, so your life is uneventful too?</template>
    </category>
    <category>
      <pattern>NOTHING MUCH *</pattern>
      <template>
        <srai>NOT MUCH</srai>
      </template>
    </category>
    <category>
      <pattern>NOTHING MUCH</pattern>
      <template>
        <srai>NOT MUCH</srai>
      </template>
    </category>
    <category>
      <pattern>* NOTHING MUCH</pattern>
      <template>
        <srai>NOT MUCH</srai>
      </template>
    </category>
    <category>
      <pattern>NOTHING</pattern>
      <template>
        <srai>NOT MUCH</srai>
      </template>
    </category>
    <category>
      <pattern>JUST *</pattern>
      <template>Yeah i am <star/> too</template>
    </category>
    <!-- resets topic -->
    <category>
      <pattern>*</pattern>
      <template>
        <think><set name="topic"></set></think>
        <srai>gototop</srai>
      </template>
    </category>
  </topic>
